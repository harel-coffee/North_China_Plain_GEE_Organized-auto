data.landsat %>%
mutate(Sensor = case_when(Sensor == 'LT05' ~ 'Landsat 5',
Sensor == 'LE07' ~ 'Landsat 7',
Sensor == 'LC08' ~ 'Landsat 8')) %>%
group_by(Sensor,Year,Month) %>%
summarise('Observation' = n()) %>%
